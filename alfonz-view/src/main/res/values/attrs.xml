<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="MaxWidthLinearLayout">
		<attr name="maxWidth" format="dimension" />
	</declare-styleable>

	<declare-styleable name="RotatableImageView">
		<attr name="angle" format="integer" />
	</declare-styleable>

	<declare-styleable name="StatefulLayout">
		<attr name="state" format="enum">
			<enum name="content" value="0" />
			<enum name="progress" value="1" />
			<enum name="offline" value="2" />
			<enum name="empty" value="3" />
		</attr>
		<attr name="progressLayout" format="reference" />
		<attr name="offlineLayout" format="reference" />
		<attr name="emptyLayout" format="reference" />
	</declare-styleable>
</resources>
